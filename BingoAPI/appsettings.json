  "IpRateLimiting": {
    "EnableEndpointRateLimiting": false,
    "StackBlockedRequests": true,
    "RealIpHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "IpWhitelist": [ /*"127.0.0.1", "::1/10"*/ ],
    "EndpointWhitelist": [],
    "ClientWhitelist": [],
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "1s",
        "Limit": 5
      },
      {
        "Endpoint": "*",
        "Period": "15m",
        "Limit": 150
      },
      {
        "Endpoint": "*",
        "Period": "12h",
        "Limit": 1000
      }
    ]
  },
  "AllowedHosts": "*",
  // Swagger 0 for disabling
  // Environment 1 for production
  "EnvironmentOptions": {
    "Environment": 0,
    "Server": "Development",
    "Swagger": 1
  }
