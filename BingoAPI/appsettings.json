    "ErrorPGConnection": "Host=localhost; Port=5432; Database=errorbingodb; Username=postgres; Password=admin;"
  },
  "JwtSettings": {
    "Secret": "",
    "TokenLifetime": "00:30:00"
  },
  "FacebookAuthSettings": {
    "AppID": "",
    "AppSecret": ""
  },
  "ApplicationEmail": {
    "EmailAddress": "administration@hopaut.com",
    "Sender": "HopAut <no-reply@hopaut.com>",
    "Password": "",
    "SmtpClient": "smtp.gmail.com",
    "Port": "587",
    "SSL": "true"
  },
  "AWSImageBucket": {
    "aws_access_key_id": "",
    "aws_secret_access_key": "",
    "bucketName": "staging-bucket420",
